#include <stdio.h>

int main(void) {
	uint32_t *regmap = (uint32_t *)0x43C00000;
	for (size_t i = 0; i < 16; i++) {
		for (size_t j = 0; j < 16; j++) {
			regmap[0] = i;
			regmap[1] = j;
			regmap[2] = 0;
			size_t s = regmap[0];
			printf("%2zu + %2zu = %3zu (%s)\n",i,j,s,(s==i+j)?"COR":"ERR");
		}
	}

	return 0;
}
